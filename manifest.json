{
    "manifest_version" : 3,
    "name": "RMP Extension",
    "description": "This is an extension that allows for quick analysis of professors",
    "version" : "1.0.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "activeTab",
        "notifications",
        "webNavigation"
        ],
    "host_permissions" : ["<all_urls>"],
    "action" : {
        "default_popup": ""
    },
    "background":{
        "service_worker": "service-worker.js"
    },
    "icons": {"128": "rmp-icon.png"}
}
