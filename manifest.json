{
    "name": "RMP Extension",
    "description": "This is an extension that allows for quick analysis of professors",
    "version" : "1.0.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "activeTab",
        "notifications",
        "webNavigation"
        ],
    "host_permissions" : ["https://*/*","http://*/*"],
    "manifest_version" : 3,
    "action" : {
        "default_popup": ""
    },
    "icons": {"128": "rmp-icon.png"},
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "all_frames": true,
      "run_at": "document_end"
}]
    }
